div.blueTable {
    border: 1px solid rgba(234,243,253,.9);
    background-color: rgb(255, 255, 255);
    width: 100%;
    text-align: left;
    border-collapse: collapse;
    color:#617182
  }
  .divTable.blueTable .divTableCell, .divTable.blueTable .divTableHead, .divTableCell {
    border: 1px solid rgba(234,243,253,.9);
    padding: 5px;
  }
  .divTable.blueTable .divTableBody .divTableCell {
    font-size: 13px;
  }
  
  .divTable.blueTable .divTableRow:nth-child(even) {
    background: #f6f9fe;
  }
  .divTable.blueTable .divTableHeading{
    background: #e9f1fc;
    border-bottom: 1px solid rgba(234,243,253,.9);
}
  .divTable.blueTable .divTableHeading .divTableHead {
    font-size: 14px;
    font-weight: bold;
    color: #617182;
    border-left: 1px solid #dee2e6;
  }
  .divTable.blueTable .divTableHeading .divTableHead:first-child {
    border-left: none;
  }

  .blueTable .tableFootStyle {
    font-size: 14px;
  }
  .blueTable .tableFootStyle .links {
       text-align: right;
  }
  .blueTable .tableFootStyle .links a{
    display: inline-block;
    background: #1C6EA4;
    color: #FFFFFF;
    padding: 2px 8px;
    border-radius: 5px;
  }
  .blueTable.outerTableFooter {
    border-top: none;
  }
  .blueTable.outerTableFooter .tableFootStyle {
    padding: 3px 5px; 
  }
  /* DivTable.com */
  .divTable{ display: table; }
  .divTableRow { display: table-row; }
  .divTableHeading { display: table-header-group;}
  .divTableCell, .divTableHead { display: table-cell;}
  .divTableHeading { display: table-header-group;}
  .divTableFoot { display: table-footer-group;}
  .divTableBody { display: table-row-group;}
  .divTableHeading .divTableRow, .divTableHead, .divTableCell p{
      padding: 2px;
      margin: 0;
  }
