//menu left thu gọn

     $(document).ready(function(){
            $(".icon_clickmenu button").click(function(){
                $('#page-wrapper').addClass("animent_menu");
                $('.bg_left_menu').addClass("menu_left");
                $('.icon_clickmenu').fadeOut();
            });
            $(".menushow").click(function(){
                $('#page-wrapper').removeClass("animent_menu");
                $('.bg_left_menu').removeClass("menu_left");
                $('.icon_clickmenu').fadeIn();
                $('.menushow').fadeOut();
            });
        });

 //end menu left thu gọn

